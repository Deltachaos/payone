{
  "name": "spryker-eco/payone",
  "autoload": {
    "psr-0": {
      "SprykerEco": "src/",
      "Function": "tests/"
    }
  },
  "require": {
    "spryker/guzzle": "^2.1.0",
    "spryker/config": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/oms": "^6.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/refund": "^4.0.0",
    "spryker/sales": "^5.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to use Checkout plugins you need to install spryker/checkout."
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker/guzzle.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/config.git"
    },
    {
      "type": "git",
      "url":  "git@github.com:spryker/kernel.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/oms.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/propel-orm.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/refund.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/sales.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/symfony.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/code-sniffer.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/propel.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/silex.git"
    },
    {
      "type": "git",
      "url": "git@github.com:spryker/testify.git"
    }
  ],
  "description": "Payone module",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  }
}
